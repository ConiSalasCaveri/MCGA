@model IEnumerable<MCGA.Entities.EspecialidadDummy>

@{
    ViewBag.Title = "EspecialidadList";
}

<h2>Especialidades del médico</h2>

<table class="table">
    <tr>        
        <th>
            @Html.DisplayNameFor(model => model.Descripcion)
        </th>        
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>        
        <td>
            @Html.DisplayFor(modelItem => item.Descripcion)
        </td>        
        <td>            
            @Html.ActionLink("Eliminar", "DeleteEspecialidad", new { id=item.Id })
        </td>
    </tr>
}

</table>
<div>
    @Html.ActionLink("Volver", "List")
</div>
